---
permalink: sitemap.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="utf-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">{% for page in collections.all %}{% if page.url != "/404.html" %}
  <url>
    <loc>{{ siteUrl }}{{ page.url }}</loc>
    <lastmod>{{ page.date | mapDate }}T00:00:00+00:00</lastmod>
  </url>{% endif %}{% endfor %}
</urlset>